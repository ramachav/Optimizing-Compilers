

;IR code
;PUSH   
;PUSHREGS   
;JSR FUNC_main  
;HALT   
;LABEL FUNC_main  
;LINK 1  
;READF num  
;STOREI 1 !T0 
;STOREI !T0 j 
;STOREF num approx 
;STOREI 100 !T1 
;STOREI !T1 i 
;LABEL WHILE_START_1  
;STOREI 0 !T2 
;EQI i !T2 WHILE_END_2
;DIVF num approx !T3
;ADDF approx !T3 !T4
;STOREF 0.500000 !T6 
;MULF !T6 !T4 !T5
;STOREF !T5 newapprox 
;STOREF newapprox approx 
;STOREI 1 !T8 
;SUBI i !T8 !T7
;STOREI !T7 i 
;JUMP WHILE_START_1  
;LABEL WHILE_END_2  
;WRITEF approx  
;WRITES eol  
;RET   
var i
var j
var newapprox
var approx
var num
str eol "\n"
push  
jsr  FUNC_main
sys halt
label  FUNC_main
link  1
sys readr num 
move 1 r0
move r0 j
move num approx
move 100 r1
move r1 i
label  WHILE_START_1
move 0 r2
cmpi  i r2
jeq  WHILE_END_2
move num r3
divr approx r3
move approx r4
addr r3 r4
move 0.500000 r6
move r6 r5
mulr r4 r5
move r5 newapprox
move newapprox approx
move 1 r8
move i r7
subi r8 r7
move r7 i
jmp  WHILE_START_1
label  WHILE_END_2
sys writer approx 
sys writes eol 
unlnk
ret
